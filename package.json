{
  "name": "umi-plugin-custom-extra",
  "main": "lib/index.js",
  "description": "extra",
  "authors": {
    "name": "kityandhero",
    "email": "kityandhero@126.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kityandhero/umi-plugin-custom-extra.git"
  },
  "scripts": {
    "start": "cross-env APP_ROOT=example umi dev",
    "build": "father-build",
    "build:p": "npm run build && npm run publish:f",
    "publish:f": "npm version patch && npm publish",
    "publish:npm": "nrm use npm && nrm ls && npm run build:p && nrm use vd && nrm ls",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:update": "umi-test --updateSnapshot"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ],
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@umijs/test": "^3.0.10",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^6.0.3",
    "father-build": "^1.17.2",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "puppeteer": "^1.20.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "test-umi-plugin": "^0.1.0",
    "umi": "^3.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "files": [
    "lib"
  ],
  "version": "1.0.10",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "umi-plugin-custom-extra"
  ],
  "author": "kityandhero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kityandhero/umijs-plugin-custom-extra/issues"
  },
  "homepage": "https://github.com/kityandhero/umijs-plugin-custom-extra#readme"
}
